<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:models="using:InternalsViewer.UI.App.Models">

    <models:MarkStyle x:Key="DefaultMarkerStyle" ForeColour="Black" BackColour="WhiteSmoke" />

    <models:MarkStyle x:Key="PageAddressMarkerStyle" ForeColour="LightBlue" BackColour="Green" Name="Page Address"/>

    <!--Header-->
    <models:MarkStyle x:Key="AllocationUnitIdMarkerStyle" ForeColour="#D50000" BackColour="#EDE7F6" Name="Allocation Unit" Ordinal="1"/>
