    <models:MarkStyle x:Key="InternalTransactionIdMarkerStyle" ForeColour="#6D4C41" BackColour="#E3F2FD" Name="Internal Transaction Id" Ordinal="170"/>

    <!--FixedVar Format-->

    <models:MarkStyle x:Key="StatusBitsAMarkerStyle" ForeColour="White" BackColour="#193960" Name="Status Bits A"/>
    <models:MarkStyle x:Key="StatusBitsBMarkerStyle" ForeColour="White" BackColour="#2C5C5B" Name="Status Bits B"/>
