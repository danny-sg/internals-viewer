    <models:MarkStyle x:Key="LongDataOffsetArrayMarkerStyle" ForeColour="White" BackColour="#B28D8A" Name="Long Data Offset Array"/>
    <models:MarkStyle x:Key="LongDataClusterArrayMarkerStyle" ForeColour="White" BackColour="#735a6d" Name="Long Data Cluster Array"/>
    <models:MarkStyle x:Key="LongFieldValueMarkerStyle" ForeColour="Black" BackColour="#e2bbe8" Name="Long Field Value"/>

    <!--Index-->

    <models:MarkStyle x:Key="UniquifierMarkerStyle" ForeColour="White" BackColour="#313240" Name="Uniquifier"/>
    <models:MarkStyle x:Key="RidMarkerStyle" ForeColour="White" BackColour="#313240" Name="RID"/>
    <models:MarkStyle x:Key="DownPagePointerMarkerStyle" ForeColour="Black" BackColour="#ffcc00" Name="Down Page Pointer"/>

    <!--Compression Info-->

    <models:MarkStyle x:Key="HeaderMarkerStyle" ForeColour="White" BackColour="#06411a" Name="Header"/>
    <models:MarkStyle x:Key="PageModificationCountMarkerStyle" ForeColour="White" BackColour="#8EBC49" Name="Page Modification Count"/>
